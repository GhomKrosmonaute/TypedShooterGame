    "style-loader": "^1.1.3",
    "typescript": "3.5.1",
    "webpack": "5.94.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "axios": "1.8.2",
    "mobile-detect": "^1.4.4",
    "moment": "^2.24.0",
    "p5": "^1.0.0",
    "querystring": "^0.2.0",
    "tims": "^1.0.4"
  }
